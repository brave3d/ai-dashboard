{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "silent": false,
    "productionBranch": "gh-pages"
  }
}
{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/generate-stream",
      "headers": {
        "Access-Control-Allow-Origin": "https://brave3d.github.io"
      }
    }
  ],
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "silent": false,
    "productionBranch": "gh-pages"
  }
}